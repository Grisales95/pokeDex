<script type="text/javascript">

    var currentPage = 0 + 1;
    var totalResult = 29;
    var totalPages = 1;
    var visiblePages = 10;
    var keyword_ran = "icon+rayo";
    var keyword_ranTmp = keyword_ran.replace("+", " ");

    $(document).ready(function () { 

        registerKeyword();
        
        var isFilter = keyword_ranTmp.split("svg png ");
        var isIcon = keyword_ranTmp.split("icon ");

        if (isFilter.length > 1) {
            $("#autocompleteSearch").val(isFilter[1]);
        } else if (isIcon.length > 1) {
            $("#autocompleteSearch").val(isIcon[1]);
        } else {
            if (keyword_ranTmp === "svg png"){
                $("#autocompleteSearch").val("");
            } else if (keyword_ranTmp === "icon"){
                $("#autocompleteSearch").val("");
            } else {
                $("#autocompleteSearch").val(keyword_ranTmp);
            }
        }
        
        //for iStock content
        validateUserLogIn("actionOnCheckLogin", "", "actionOnCheckLogin", "");

        $('.results-pagination').twbsPagination({
            totalPages: totalPages,
            visiblePages: visiblePages,
            startPage: currentPage,
            //href: '',
            onPageClick: function (event, page) {
                //alert(page);
                window.location.href = "https://es.vexels.com/icon/rayo/p/" + page + "/";
                //$('#page-content').text('Page ' + page);                  
            }
        });  
        
    });

    function actionOnCheckLogin(){
        consoleLog("logged user is: " + loggedUserId);
        if(!user_has_licence && !isSubscriptor){
            $(".content-istock").show();
            consoleLog("making istock search with: " + keyword_ran);
            makeIStockSearchSearchPage(keyword_ran, "", currentPage, 10);
        } else{
            consoleLog("User premium or licenced, hidding iStock");
            $(".content-istock").hide();
        }
    }

    function registerKeyword(){

        consoleLog("registering keyword");

        if($("#resultsFound").html() == '') $("#resultsFound").html('0');

        $.ajax({
            url:    '/ajax/vexels-api/register-search-keyword-results/',
            type:     'post',
            data:     {keyword: $("#wordText").html(), results: $("#resultsFound").html()},
            beforeSend: function(request) {
                request.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
            },
            statusCode: {
                  404: function() {
                    consoleLog('Could not contact server.');
                  },
                  500: function() {
                    consoleLog('A server-side error has occurred.');
                  }
            },
            success:  function(data) {
                consoleLog(JSON.stringify(data));
            },
            fail:     function(error){
                console.info(JSON.stringify(error));
            }
        });
    }
</script>
