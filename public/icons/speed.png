        $('#step-2-hidden').hide();
        $('#step-2-login-hidden').show();
        $('#step-3-hidden').show();
    });
    

    $('.pay_tab_box_btns a').click(function(){
            
        if(sessionStorage.getItem("step_plans")==2){   
            $('#step-2-hidden').show();
            $('#step-2-login-hidden').hide();
            $('#step-3-hidden').hide();
        }
        else{  
            $('#step-2-hidden').hide();
            $('#step-2-login-hidden').show();
            $('#step-3-hidden').show();
        }  

        setTimeout(function(){            
            if(globalUserIsLogged){   
                sessionStorage.setItem("step_plans", 2);
            }
            else{  
                sessionStorage.setItem("step_plans", 3);
            }    
        }, 1000);
    })

    $('.pricing').click(function(){
        if(globalUserIsLogged){            
            sessionStorage.setItem("step_plans", 2);
        }
        else{
            sessionStorage.setItem("step_plans", 3);
        }
    })

    if($('#step-1-hidden').length){
        setTimeout(function(){
            if(sessionStorage.getItem("step_plans")==2){
                $('#step-2-hidden').show();
