var validating = false;

function toggleValidatingBlur(){
    if(!validating){
        $(".validando-tarjeta").fadeIn('500');
        validating = true;
        $("#validator__aux_div").addClass('validando-tarjeta-fondo');
    } else{
        $(".validando-tarjeta").fadeOut('1500');
        validating = false;
        $("#validator__aux_div").removeClass('validando-tarjeta-fondo');
    }
}

function showConfirmPurchasePanel(type){

    var text = type=='paypal'?"You'll be redirected to Paypal. Confirm purchase?":"You order will be processed. Confirm Purchase?";
    var panel = getConfirmPanelHTML(type, text, "goTo_" + type + "();", "cancelPurchase('" + type + "');");
    
    setTimeout("goTo_" + type + "();", 0);
}

function cancelPurchase(type){
    
    $("#confirm_panel_" + type).fadeOut('1500', function(){
        if(type == "paypal"){
            $("#paypal_button").fadeIn('1500');
        } else{
            $("#bluesnap_button").fadeIn('1500');        
        }
        $("#confirm_panel_" + type).remove();
    });
}

function getConfirmPanelHTML(type, question_text, yes_action, no_action){

    var html = "";

    html += '<div id="confirm_panel_' + type + '" class="confirm_purchase_panel">';
    html += '   <div class="confirm__purchase_text">' + question_text + '</div>';
    html += '   <div class="confirm_buttons_container">';
    html += '       <a class="confirm__purchase_yes_btn clickable" style="display:none;" id="yes_button_' + type + '" onclick="' + yes_action + '">Yes</a>';
    html += '       <div class="confirm__or">or</div>';
    html += '       <a class="confirm__purchase_no_btn clickable" style="display:none;" id="no_button_' + type + '" onclick="' + no_action + '">No</a>';
    html += '   </div>';
    html += '</div>';

    return html;
}

function dismissPrompt(obj){
     $(obj).fadeOut(function(){
        $('#no_more_label').fadeOut();
        dismissPendingNotification('prompt_notification', true);
     });
}

